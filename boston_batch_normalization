      "source": [
        "import tensorflow as tf\n",
        "import pandas as pd\n",
        "fileRoute='https://raw.githubusercontent.com/blackdew/tensorflow1/master/csv/boston.csv'\n",
        "boston=pd.read_csv(fileRoute)\n",
        "x=boston[['crim','zn','indus','chas','nox','rm','age','dis','rad','tax','ptratio','b','lstat']]\n",
        "y=boston[['medv']]\n",
        "print(x.shape, y.shape)\n",
        "\n",
        "X=tf.keras.layers.Input(shape=[13])#13개의 독립변수\n",
        "\n",
        "H=tf.keras.layers.Dense(8)(X)\n",
        "H=tf.keras.layers.BatchNormalization()(H)\n",
        "H=tf.keras.layers.Activation('swish')(H)\n",
        "\n",
        "H=tf.keras.layers.Dense(8)(H)\n",
        "H=tf.keras.layers.BatchNormalization()(H)\n",
        "H=tf.keras.layers.Activation('swish')(H)\n",
        "\n",
        "H=tf.keras.layers.Dense(8)(H)\n",
        "H=tf.keras.layers.BatchNormalization()(H)\n",
        "H=tf.keras.layers.Activation('swish')(H)\n",
        "\n",
        "Y=tf.keras.layers.Dense(1)(H)#1개의 종속변수,X의 종속변수\n",
        "model=tf.keras.models.Model(X,Y)#X,Y의 모델 생성\n",
        "model.compile(loss='mse')\n",
        "model.fit(x,y,epochs=1000,verbose=0) #모델이 X,Y로 이루어져있으니 데이터셋 x,y로 학습시킴\n",
        "print(model.predict(x[0:5]))#x데이터셋 0번부터 5개를 예측\n",
        "y[0:5]#실제 종속값\n",
        "\n"
      ]
    }
  ]
}